   <string notr="true">/* Apply to the entire application */
QWidget {
    
    
    font-family: Arial, sans-serif; /* Set font to Arial or a fallback sans-serif */
}

/* Apply to buttons */
QPushButton {
    background-color: #4A90E2; /* Light blue background */
    border: none; /* No border */
    color: white; /* White text color */
    padding: 10px 20px; /* Padding */
    border-radius: 5px; /* Rounded corners */
    font-size: 14px; /* Font size */
}

QPushButton:hover {
    background-color: #357EBD; /* Darker blue on hover */
}

/* Apply to labels */
QLabel {
    color: #333333; /* Dark text color */
    font-size: 16px; /* Font size */
}

/* Apply to group boxes */
QGroupBox {
    background-color: #ffffff; /* White background */
    border: 2px solid #4A90E2; /* Light blue border */
    border-radius: 5px; /* Rounded corners */
}

/* Apply to sliders */
QSlider::groove:horizontal {
    border: 1px solid #999999; /* Gray border */
    background: #ffffff; /* White background */
    height: 10px; /* Groove height */
    border-radius: 5px; /* Rounded corners */
}

QSlider::handle:horizontal {
    background: #4A90E2; /* Light blue handle */
    border: 1px solid #4A90E2; /* Light blue border */
    width: 20px; /* Handle width */
    margin: -5px 0; /* Center the handle vertically */
    border-radius: 10px; /* Rounded corners */
}

/* Apply to combo boxes */
QComboBox {
    background-color: #ffffff; /* White background */
    border: 1px solid #4A90E2; /* Light blue border */
    border-radius: 5px; /* Rounded corners */
    padding: 5px; /* Padding */
}

/* Apply to graphics views */
QGraphicsView {
    background-color: #ffffff; /* White background */
    border: 1px solid #4A90E2; /* Light blue border */
    border-radius: 5px; /* Rounded corners */
}

/* Apply to scroll bars */
QScrollBar:vertical {
    border: 1px solid #999999; /* Gray border */
    background: #ffffff; /* White background */
    width: 10px; /* Scroll bar width */
    margin: 0px 0px 0px 0px; /* Margin */
}

QScrollBar::handle:vertical {
    background: #4A90E2; /* Light blue handle */
    min-height: 20px; /* Handle minimum height */
    border-radius: 5px; /* Rounded corners */
}

QScrollBar::add-line:vertical {
    height: 0px; /* No height for add line */
    subcontrol-position: bottom; /* Align to bottom */
    subcontrol-origin: margin; /* Origin from margin */
}

QScrollBar::sub-line:vertical {
    height: 0px; /* No height for sub line */
    subcontrol-position: top; /* Align to top */
    subcontrol-origin: margin; /* Origin from margin */
}
